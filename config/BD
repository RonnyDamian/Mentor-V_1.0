DROP DATABASE IF EXISTS pryEncuesta;
CREATE DATABASE IF NOT EXISTS pryEncuesta;
use pryEncuesta;
/*Creación tabla usuarios*/
DROP TABLE IF EXISTS t_usuarios;
CREATE TABLE IF NOT EXISTS t_usuarios
(

id_Usuario INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
nombre VARCHAR(70) NOT NULL,
apellido VARCHAR(70) NOT NULL,
sexo VARCHAR(15) NOT NULL,
edad VARCHAR(3) NOT NULL,
celular VARCHAR(10) NOT NULL,
email VARCHAR(100) NOT NULL,
empresa VARCHAR(100) NOT NULL,
cargo VARCHAR(50) NOT NULL
)ENGINE=INNODB DEFAULT CHARSET="utf8";
/*Creación tabla animoso*/
DROP TABLE IF EXISTS t_animodo;
CREATE TABLE IF NOT EXISTS t_animodo
(

idEncuesta INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
preg1 INT(11)NOT NULL,
preg2 INT(11)NOT NULL,
preg3 INT(11)NOT NULL,
preg4 INT(11)NOT NULL,
preg5 INT(11)NOT NULL,
preg6 INT(11)NOT NULL,
preg7 INT(11)NOT NULL,
preg8 INT(11)NOT NULL,
preg9 INT(11)NOT NULL,
preg10 INT(11)NOT NULL,
preg11 INT(11)NOT NULL,
preg12 INT(11)NOT NULL,
preg13 INT(11)NOT NULL,
preg14 INT(11)NOT NULL,
id_Usuario INT(11) NOT NULL,
animodo VARCHAR (70) NOT NULL
)ENGINE=INNODB DEFAULT CHARSET="utf8";



ALTER TABLE t_encuesta ADD CONSTRAINT FOREIGN KEY(id_Usuario) REFERENCES t_usuarios(id_Usuario) ON DELETE CASCADE ON UPDATE CASCADE;

