/*Creación de la tabla tipos de cerebro*/
DROP TABLE IF EXISTS t_tiposCerebro;
CREATE TABLE IF NOT EXISTS t_tiposCerebro
(

idEncuesta INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
preg1 INT(3)NOT NULL,
preg2 INT(3)NOT NULL,
preg3 INT(3)NOT NULL,
preg4 INT(3)NOT NULL,
preg5 INT(3)NOT NULL,
preg6 INT(3)NOT NULL,
preg7 INT(3)NOT NULL,
preg8 INT(3)NOT NULL,
preg9 INT(3)NOT NULL,
preg10 INT(3)NOT NULL,
preg11 INT(3)NOT NULL,
preg12 INT(3)NOT NULL,
preg13 INT(3)NOT NULL,
preg14 INT(3)NOT NULL,
preg15 INT(3)NOT NULL,
preg16 INT(3)NOT NULL,
preg17 INT(3)NOT NULL,
preg18 INT(3)NOT NULL,
preg19 INT(3)NOT NULL,
preg20 INT(3)NOT NULL,
preg21 INT(3)NOT NULL,
preg22 INT(3)NOT NULL,
preg23 INT(3)NOT NULL,
cerebroIzq VARCHAR (8) NOT NULL,
cerebroCen VARCHAR (8) NOT NULL,
cerebroDer VARCHAR (8) NOT NULL,
valoracion INT(3) NOT NULL,
id_Usuario INT(11) NOT NULL

)ENGINE=INNODB DEFAULT CHARSET="utf8";

/*Crecion tabla evaluación negociadora*/
DROP TABLE IF EXISTS t_evalNegociadora;
CREATE TABLE IF NOT EXISTS t_evalNegociadora
(

idEncuesta INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
preg1 INT(3)NOT NULL,
preg2 INT(3)NOT NULL,
preg3 INT(3)NOT NULL,
preg4 INT(3)NOT NULL,
preg5 INT(3)NOT NULL,
preg6 INT(3)NOT NULL,
preg7 INT(3)NOT NULL,
preg8 INT(3)NOT NULL,
preg9 INT(3)NOT NULL,
preg10 INT(3)NOT NULL,
preg11 INT(3)NOT NULL,
preg12 INT(3)NOT NULL,
preg13 INT(3)NOT NULL,
preg14 INT(3)NOT NULL,
preg15 INT(3)NOT NULL,
preg16 INT(3)NOT NULL,
preg17 INT(3)NOT NULL,
preg18 INT(3)NOT NULL,
preg19 INT(3)NOT NULL,
preg20 INT(3)NOT NULL,
preg21 INT(3)NOT NULL,
preg22 INT(3)NOT NULL,
preg23 INT(3)NOT NULL,
preg24 INT(3)NOT NULL,
preg25 INT(3)NOT NULL,
preg26 INT(3)NOT NULL,
preg27 INT(3)NOT NULL,
preg28 INT(3)NOT NULL,
preg29 INT(3)NOT NULL,
preg30 INT(3)NOT NULL,
cantidad INT(3)NOT NULL,
valoracion VARCHAR (10) NOT NULL,
cualidad VARCHAR (20) NOT NULL,
id_Usuario INT(11) NOT NULL
)ENGINE=INNODB DEFAULT CHARSET="utf8";
/*Creación de la  tabla TEST DE VAK*/
DROP TABLE IF EXISTS t_testVak;
CREATE TABLE IF NOT EXISTS t_testVak
        (

        idEncuesta INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
        preg1 INT(11)NOT NULL,
        preg2 INT(11)NOT NULL,
        preg3 INT(11)NOT NULL,
        preg4 INT(11)NOT NULL,
        preg5 INT(11)NOT NULL,
        preg6 INT(11)NOT NULL,
        preg7 INT(11)NOT NULL,
        preg8 INT(11)NOT NULL,
        preg9 INT(11)NOT NULL,
        preg10 INT(11)NOT NULL,
        preg11 INT(11)NOT NULL,
        preg12 INT(11)NOT NULL,
        preg13 INT(11)NOT NULL,
        preg14 INT(11)NOT NULL,
        id_Usuario INT(11) NOT NULL,
        animodo VARCHAR (70) NOT NULL
        )ENGINE=INNODB DEFAULT CHARSET="utf8";


/*Creación de la tabla Nivel de Persistencia*/

DROP TABLE IF EXISTS t_nivelPersistencia;
CREATE TABLE IF NOT EXISTS t_nivelPersistencia
        (
        idEncuesta INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
        colorComunicacion INT(3)NOT NULL,
        tiposCerebro INT(3)NOT NULL,
        formaNegociadora INT(3)NOT NULL,
        valoracion INT(3) NOT NULL,
        persistente VARCHAR(5) NOT NULL,
        id_Usuario INT(11) NOT NULL
        )ENGINE=INNODB DEFAULT CHARSET="utf8";
